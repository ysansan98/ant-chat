{
  "name": "electron",
  "type": "module",
  "version": "1.0.0-alpha.0",
  "description": "Ant Chat Electron App",
  "author": {
    "email": "ysansan.tech@icloud.com",
    "name": "幺叁叁"
  },
  "main": "out/main/index.cjs",
  "scripts": {
    "start": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "dev": "electron-vite dev",
    "prebuild": "electron-vite build",
    "type-check": "tsc -b",
    "test:unit": "vitest -c ./vitest.config.ts",
    "rebuild:electron": "pnpm exec electron-rebuild -f -w better-sqlite3",
    "rebuild:node": "pnpm rebuild better-sqlite3",
    "build:unpack": "cross-env NODE_ENV=production electron-vite build && electron-builder --dir",
    "build:win": "cross-env NODE_ENV=production electron-vite build && electron-builder --win",
    "build:mac": "cross-env NODE_ENV=production electron-vite build && electron-builder --mac",
    "build:linux": "cross-env NODE_ENV=production electron-vite build && electron-builder --linux",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "cross-env NODE_ENV=development tsx src/main/db/migrate.ts"
  },
  "dependencies": {
    "@google/genai": "^1.3.0",
    "better-sqlite3": "^11.10.0",
    "drizzle-orm": "^0.40.0",
    "electron-log": "^5.4.0",
    "electron-store": "^8.0.0",
    "electron-updater": "^6.2.0",
    "motion": "^12.23.5",
    "openai": "^5.2.0",
    "undici": "^7.15.0"
  },
