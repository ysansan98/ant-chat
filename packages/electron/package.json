    "@vitest/ui": "3.1.1",
    "ahooks": "^3.8.4",
    "antd": "^5.24.9",
    "babel-plugin-react-compiler": "19.1.0-rc.1",
    "code-inspector-plugin": "^0.20.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.11",
    "drizzle-kit": "^0.31.1",
    "drizzle-zod": "^0.7.1",
    "electron": "^36.9.1",
    "electron-builder": "^26.0.0",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^4.0.0",
    "immer": "^10.1.1",
